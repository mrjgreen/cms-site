{
  "name": "victor-hugo",
  "version": "1.0.0",
  "description": "Victor Hugo is a Hugo boilerplate for creating truly epic websites!",
  "repository": "netlify/victor-hugo",
  "main": "index.js",
  "scripts": {
    "start": "hugo -d ../dist -s site -vw",
    "preview": "npm run start:hugo -- -D -F",
    "prebuild": "rm -fr dist",
    "build": "hugo -d ../dist -s site -v",
    "build:preview": "npm run build:hugo -- -D -F"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "hugo-bin": "^0.37.0"
  },
  "resolutions": {
    "natives": "1.1.6"
  }
}
